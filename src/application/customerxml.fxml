      <RadioButton fx:id="male" layoutX="244.0" layoutY="226.0" mnemonicParsing="false" text="Male" />
      <TextField fx:id="address" layoutX="244.0" layoutY="292.0" text="Address" />
      <TextField fx:id="city" layoutX="244.0" layoutY="340.0" text="City" />
